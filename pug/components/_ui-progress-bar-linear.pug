//- progress - object{progress}
mixin progress-bar-linear(progress)
	// Linear progress bar
	article.progress-linear
		.progress-header
			p= progress.title
			span.progress-value= progress.valueTo
		.progress-bar-linear-wrap
			if(!progress.progressBarColor)
				.progress-bar-linear
			else
				.progress-bar-linear(class=progress.progressBarColor)
